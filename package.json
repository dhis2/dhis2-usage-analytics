{
  "name": "usage-analytics-app",
  "version": "32.0.1",
  "private": true,
  "dependencies": {
    "@dhis2/ui": "1.0.0-beta.14",
    "chart.js": "2.7.3",
    "core-js": "3.0.1",
    "lodash.debounce": "4.0.8",
    "react": "^16.8.6",
    "react-app-polyfill": "0.2.2",
    "react-chartjs-2": "2.7.4",
    "react-dom": "^16.8.6",
    "react-redux": "6.0.0",
    "react-scripts": "3.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "REACT_APP_DHIS2_BASE_URL=http://localhost:8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n usage-analytics-app -p ./i18n/ -o ./src/locales/",
    "manifest": "d2-manifest manifest.json ./public/manifest.webapp",
    "prestart": "yarn localize && yarn manifest",
    "prebuild": "yarn localize && yarn manifest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "ie 11",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@dhis2/d2-i18n": "1.0.4",
    "@dhis2/d2-i18n-extract": "1.0.7",
    "@dhis2/d2-i18n-generate": "1.0.19",
    "@dhis2/packages": "1.3.0",
    "bfj": "6.1.1",
    "d2-manifest": "^1.0.0",
    "fetch-mock": "^7.3.0",
    "identity-obj-proxy": "3.0.0",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  }
