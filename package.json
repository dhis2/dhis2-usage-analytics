{
  "name": "usage-analytics-app",
  "version": "32.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:style": "d2-style js check --all",
    "eject": "react-scripts eject",
    "release": "packages release",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n usage-analytics-app -p ./i18n/ -o ./src/locales/",
    "prestart": "yarn localize",
    "prebuild": "yarn localize",
    "pretest": "yarn localize"
  },
  "dependencies": {
    "@dhis2/ui": "^1.0.0-beta.14",
    "chart.js": "^2.7.3",
    "core-js": "^3.0.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "^3.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^3.1.3",
    "@dhis2/d2-i18n": "^1.0.4",
    "@dhis2/d2-i18n-extract": "^1.0.7",
    "@dhis2/d2-i18n-generate": "^1.1.1",
    "@dhis2/packages": "^1.3.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "husky": "^2.2.0",
    "jest-enzyme": "^7.0.2",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",	
      "!src/**/*.d.ts",	
      "!src/serviceWorker.js",	
      "!src/index.js",	
      "!src/store.js",	
      "!src/__mockData__/**/*.js"
    ]
  }
}
