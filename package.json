{
  "name": "usage-analytics-app",
  "version": "32.0.1",
  "private": true,
  "scripts": {
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n usage-analytics-app -p ./i18n/ -o ./src/locales/",
    "manifest": "d2-manifest manifest.json ./public/manifest.webapp",
    "format": "packages code-style",
    "release": "packages release",
    "prestart": "yarn localize && yarn manifest",
    "start": "node scripts/start.js",
    "prebuild": "rm -rf build && yarn format && mkdir build && yarn manifest",
    "build": "yarn localize && node scripts/build.js",
    "test": "node scripts/test.js",
    "coverage": "npm test -- --coverage"
  },
  "husky": {
    "hooks": {
      "commit-msg": "packages commit-style",
      "pre-commit": "packages code-style && yarn extract-pot && CI=true yarn test && git add ./i18n/"
    }
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@dhis2/ui": "^1.0.0-beta.7",
    "chart.js": "^2.7.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@dhis2/d2-i18n": "^1.0.4",
    "@dhis2/d2-i18n-extract": "^1.0.7",
    "@dhis2/d2-i18n-generate": "^1.0.19",
    "@dhis2/packages": "^1.3.0",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "2.0.0",
    "d2-manifest": "^1.0.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.10.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^1.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-enzyme": "^7.0.1",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.4.0",
    "postcss-rtl": "^1.3.2",
    "postcss-safe-parser": "4.0.1",
    "react-dev-utils": "^6.1.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-dev-server": "3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  }
}
